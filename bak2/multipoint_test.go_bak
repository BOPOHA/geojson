package geojson

import "testing"

func TestMultiPoint(t *testing.T) {
	mustParseGeoJSON(
		`{"type":"MultiPoint","coordinates":[[10,11],[12,13]]}`,
	)
	mustParseGeoJSON(
		`{"type":"MultiPoint","coordinates":[[10,11,12],[13,14,15]]}`,
	)
	mustParseGeoJSON(
		`{"type":"MultiPoint","coordinates":[]}`,
	)
	mustParseGeoJSON(
		`{"type":"MultiPoint","coordinates":[[10,11,13,14],[15,16,17,18]]}`,
	)
	mustParseGeoJSON(
		`{"type":"MultiPoint","coordinates":[[10,11]],"bbox":[1,2,3,4]}`,
	)
	mustParseGeoJSON(
		`{"type":"MultiPoint","coordinates":[[10,11,12]],"bbox":[1,2,3,4]}`,
	)
	mustParseGeoJSON(
		`{"type":"MultiPoint","coordinates":[[10,11,12,13]],"bbox":[1,2,3,4]}`,
	)
	mustParseGeoJSON(
		`{"type":"MultiPoint","coordinates":[[10,11,12,13]],"bbox":[1,2,3,4,5,6]}`,
	)
	mustParseGeoJSON(
		`{"type":"MultiPoint","coordinates":[[10,11,12,13]],"bbox":[1,2,3,4,5,6,7,8]}`,
	)
}
